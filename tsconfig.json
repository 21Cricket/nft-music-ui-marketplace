{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "allowJs": false,
        "skipLibCheck": true,
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "allowImportingTsExtensions": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "outDir": "../../../../.moon/cache/types/apps/web/front/webfrontclient"
    },
    "include": [
        "src"
    ],
    "references": [
        {
            "path": "../../../../shared/front"
        },
        {
            "path": "../../../../shared/pckgs/browser_pckg"
        },
        {
            "path": "./tsconfig.node.json"
        }
    ]
}
